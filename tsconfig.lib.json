{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,

    "outDir": "dist",
    "rootDir": "src",

    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",

    "strict": true,
    "jsx": "react-jsx",

    "skipLibCheck": true
  },
  "include": ["src"]
}